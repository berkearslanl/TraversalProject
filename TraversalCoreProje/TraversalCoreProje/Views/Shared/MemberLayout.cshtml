@inject Microsoft.AspNetCore.Mvc.Localization.IViewLocalizer localizer
<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
@await Component.InvokeAsync("MemberLayoutHeader")
<body class="vertical-layout vertical-menu 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-menu" data-color="bg-gradient-x-purple-blue" data-col="2-columns">

    <nav class="header-navbar navbar-expand-md navbar navbar-with-menu navbar-without-dd-arrow fixed-top navbar-semi-light">
        <div class="navbar-wrapper">
            <div class="navbar-container content">
                <div class="collapse navbar-collapse show" id="navbar-mobile">
                    <ul class="nav navbar-nav mr-auto float-left">
                        <li class="nav-item d-block d-md-none"><a class="nav-link nav-menu-main menu-toggle hidden-xs" href="#"><i class="ft-menu"></i></a></li>
                        <li class="nav-item d-none d-md-block"><a class="nav-link nav-link-expand" href="#"><i class="ficon ft-maximize"></i></a></li>
                        <li class="nav-item dropdown navbar-search">
                            <a class="nav-link dropdown-toggle hide" data-toggle="dropdown" href="#"><i class="ficon ft-search"></i></a>
                            <ul class="dropdown-menu">
                                <li class="arrow_box">
                                    <form>
                                        <div class="input-group search-box">
                                            <div class="position-relative has-icon-right full-width">
                                                <input class="form-control" id="search" type="text" placeholder="Ara...">
                                                <div class="form-control-position navbar-search-close"><i class="ft-x">   </i></div>
                                            </div>
                                        </div>
                                    </form>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    @await Component.InvokeAsync("MemberLayoutLanguages")
                    @await Component.InvokeAsync("MemberLayoutNavbar")
                    
                </div>
            </div>
        </div>
    </nav>

    @* @await Component.InvokeAsync("MemberLayoutLeftBar") *@

    <div class="main-menu menu-fixed menu-light menu-accordion    menu-shadow " data-scroll-to-active="true" data-img="/chameleon-admin-free-bootstrap-dashboard-template-v1.0/theme-assets/images/backgrounds/02.jpg">
        <div class="navbar-header">
            <ul class="nav navbar-nav flex-row">
                <li class="nav-item mr-auto">
                    <a class="navbar-brand" href="index.html">
                        <img class="brand-logo" alt="Chameleon admin logo" src="/chameleon-admin-free-bootstrap-dashboard-template-v1.0/theme-assets/images/logo/logo.png" />
                        <h3 class="brand-text">Traversal</h3>
                    </a>
                </li>
                <li class="nav-item d-md-none"><a class="nav-link close-navbar"><i class="ft-x"></i></a></li>
            </ul>
        </div>
        <div class="main-menu-content">
            <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">
                <li class=" nav-item">
                    <a href="/Member/Dashboard/MemberDashboard/"><i class="ft-home"></i><span class="menu-title" data-i18n="">@localizer["Dashboard"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Member/Profil/Index/"><i class="ft-pie-chart"></i><span class="menu-title" data-i18n="">@localizer["Profil"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Member/Rezervasyon/BenimAktifRezervasyonlarim/"><i class="ft-droplet"></i><span class="menu-title" data-i18n="">@localizer["Aktif Rezv."]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Member/Rezervasyon/BenimOnayBekleyenRezervasyonlarim/"><i class="ft-layers"></i><span class="menu-title" data-i18n="">@localizer["Onay Bekleyen Rezv."]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Member/Rezervasyon/BenimGecmisRezervasyonlarim/"><i class="ft-box"></i><span class="menu-title" data-i18n="">@localizer["Geçmiş Rezv."]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Member/Rezervasyon/YeniRezervasyon/"><i class="ft-book"></i><span class="menu-title" data-i18n="">@localizer["Yeni Rezervasyon"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Member/Rotalar/Index/"><i class="ft-bold"></i><span class="menu-title" data-i18n="">@localizer["Tur Rotaları"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="tables.html"><i class="ft-credit-card"></i><span class="menu-title" data-i18n="">@localizer["Yorumlarım"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Main/Index/"><i class="ft-layout"></i><span class="menu-title" data-i18n="">@localizer["Siteye Git"]</span></a>
                </li>
                <li class=" nav-item">
                    <a href="/Login/CikisYap/"><i class="ft-power"></i><span class="menu-title" data-i18n="">@localizer["Çıkış Yap"]</span></a>
                </li>
            </ul>
        </div>
        <div class="navigation-background"></div>
    </div>
    @*--------------------------------------------------------*@

    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            <div class="content-header row">
                <div class="content-header-left col-md-4 col-12 mb-2">
                    <h3 class="content-header-title">Traversal</h3>
                </div>
                <div class="content-header-right col-md-8 col-12">
                    <div class="breadcrumbs-top float-md-right">
                        <div class="breadcrumb-wrapper mr-1">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item">
                                    <a href="/Main/Index/">Ana Sayfa</a>
                                </li>
                                <li class="breadcrumb-item active">
                                    Üye Sayfası
                                </li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
            <div class="content-body">
                
                @RenderBody()
            </div>
        </div>
    </div>
    
    @await Component.InvokeAsync("MemberLayoutFooter");

    <script src="~/chameleon-admin-free-bootstrap-dashboard-template-v1.0/theme-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <script src="~/chameleon-admin-free-bootstrap-dashboard-template-v1.0/theme-assets/js/core/app-menu-lite.js" type="text/javascript"></script>
    <script src="~/chameleon-admin-free-bootstrap-dashboard-template-v1.0/theme-assets/js/core/app-lite.js" type="text/javascript"></script>
</body>
</html>